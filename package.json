  "license": "MIT",
  "scripts": {
    "type-check": "tsc",
    "build": "tsc --project tsconfig.json && env-cmd -f .env gatsby build",
    "develop": "gatsby develop",
    "gatsby-play": "env-cmd -f .env gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing \"",
    "lint": "eslint 'src/**/*.{ts,tsx}' --max-warnings=0 --ignore-path .gitignore . --ext ts --ext tsx --ext js --ext jsx",
    "lint:fix": "eslint 'src/**/*.{ts,tsx}' --max-warnings=0 --fix"
  }
